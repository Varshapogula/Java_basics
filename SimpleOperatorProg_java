class Operators {
  public static void main(String[] args) {
    int a = 15, b = 10;
    // addition operator
    System.out.println("a + b = " + (a + b));
    // subtraction operator
    System.out.println("a - b = " + (a - b));
    // multiplication operator
    System.out.println("a * b = " + (a * b));
    // division operator
    System.out.println("a / b = " + (a / b));
    // modulo operator
    System.out.println("a % b = " + (a % b));
  }
}
